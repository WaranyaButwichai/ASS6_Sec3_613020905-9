<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OneFragment">

    <ImageView
        android:id="@+id/image_eed"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:src="@drawable/itsme"
        android:layout_centerInParent="true" />
    <TextView
        android:id="@+id/name_eed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Waranya Butwichai"
        android:textSize="30sp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/image_eed"/>
    <TextView
        android:id="@+id/id_eed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="613020905-9"
        android:textSize="30sp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/name_eed"/>

</RelativeLayout>